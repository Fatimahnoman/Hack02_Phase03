{
  "version": 2,
  "name": "evolution-of-todo-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "serverless": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://fatimahnoman-phase-three.hf.space/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://fatimahnoman-phase-three.hf.space"
  }
}